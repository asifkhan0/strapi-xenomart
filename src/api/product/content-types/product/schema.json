{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "required": true,
      "type": "decimal"
    },
    "description": {
      "type": "blocks"
    },
    "rating": {
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "new arrival",
        "men",
        "women",
        "shirt",
        "jeans",
        "paint",
        "shoes"
      ]
    },
    "offer_price": {
      "type": "decimal"
    },
    "instantDelivery": {
      "type": "boolean"
    }
  }
}
